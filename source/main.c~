#include <stdio.h>

int main(int argc, char** argv)
{
	if (argc < 5 || !isdigit(argv[2][0)] || !isdigit(argv[4][0]))
		printf("USAGE\n\t./philo -p nbr_p -e nbr_e\n\nDESCRIPTION\n\t-p nbr_p\t number of philosophers\n\t-e nbr_e\t maximum number times a philosopher eat before exiting the program\n");
}
